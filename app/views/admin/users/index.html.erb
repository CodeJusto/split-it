<h1>Admin User Management</h1>
<div><%= link_to "New user", new_admin_user_path %></div>
<hr>
<% @users.each do |user| %>
  <p><%= user.name %> - <%= user.email %> | 
    <%= link_to "view", admin_user_path(user) %> | 
    <%= link_to "edit", edit_admin_user_path(user) %> | 
    <%= link_to "delete", admin_user_path(user), method: :delete, data: {confirm: "You sure?", test: "1"} %>
  </p>
  <hr>
<% end %>